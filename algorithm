System.setProperty("webdriver.chrome.driver", "path/to/chromedriver"); // Altere para o caminho do seu ChromeDriver

        // Inicializar o navegador
        WebDriver driver = new ChromeDriver();

        try {
            // Navegar para o site onde deseja adicionar os cookies
            driver.get("https://github.com");

            // Criar e adicionar cookies
            Cookie cookie1 = new Cookie.Builder("user_test", "Nf49uRwaB0eqBeW7pV9YYP_Sz_0nWSAa50QFtmyUxiZvW9Pm")
                .domain(".github.com")
                .path("/")
                .expiresOn(new java.util.Date(1719839632L * 1000))
                .isHttpOnly(false)
                .isSecure(false)
                .build();

            driver.manage().addCookie(cookie1);

            // Recarregar a página para aplicar os cookies
            driver.navigate().refresh();

            // Verificar se os cookies foram adicionados
            for (Cookie cookie : driver.manage().getCookies()) {
                System.out.println(cookie);
            }

            // Manter o navegador aberto para inspeção
            Thread.sleep(5000);
        } catch (Exception e) {
            e.printStackTrace();
        } finally {
            // Fechar o navegador
            driver.quit();
        }
