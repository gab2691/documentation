# Prevent the computer from going idle by moving the mouse cursor every 5 seconds
while ($true) {
    [System.Windows.Forms.Cursor]::Position = New-Object System.Drawing.Point(0, 0)
    Start-Sleep -Seconds 1
    [System.Windows.Forms.Cursor]::Position = New-Object System.Drawing.Point(10, 10)
    Start-Sleep -Seconds 5
}
powershell -ExecutionPolicy Bypass -File KeepTeamsOnline.ps1
