@Table(name = "tDmandProvs")
public class DemandaReservaProvisionada {

    @Id
    @Column(name = "cDmandProvs")
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;

    @Column(name = "cSist", columnDefinition = "CHAR(4)")
    private String codSistema;

    @Column(name = "iSglDomnoVila", length = 10)
    private String siglaDominioVila;

    @Column(name = "iNomeProj", length = 50)
    private String nomeProjeto;

    @Column(name = "iVlrFluxo", length = 50)
    private String valorFluxo;

    @Column(name = "cEmailUsuar", columnDefinition = "VARCHAR(150)")
    private String emailUsuario;

    @Column(name = "hDataHoraProvs")
    private LocalDateTime dataHoraProvisionamento;

    @ManyToOne
    @JoinColumn(name = "cDmandReserEstrt", nullable = false)
    private DemandaReserva demanda;

    @Column(name = "cUsuarManut", columnDefinition = "CHAR(9)")
    private String codFuncional;
}
