1000 *
sum by (integration, entityType, generationType, executionId) (
  increase(process_files_duration_seconds_sum[$__range])
)
/
sum by (integration, entityType, generationType, executionId) (
  increase(process_files_duration_seconds_count[$__range])
)
sum by (integration, entityType, generationType, executionId) (
  increase(process_files_rows_total[$__range])
)
