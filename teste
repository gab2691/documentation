kubectl apply -f https://raw.githubusercontent.com/external-secrets/external-secrets/v0.9.13/deploy/crds/bundle.yaml
kubectl get crd externalsecrets.external-secrets.io -o=jsonpath='{range .spec.versions[*]}{.name}={.served}{"\n"}{end}'
kubectl get crd secretstores.external-secrets.io -o=jsonpath='{range .spec.versions[*]}{.name}={.served}{"\n"}{end}'
