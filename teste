1000 *
sum by (integration, entityType, generationType) (
  rate(process_files_duration_seconds_sum[$__rate_interval])
)
/
sum by (integration, entityType, generationType) (
  rate(process_files_duration_seconds_count[$__rate_interval])
)
sum by (integration, entityType, generationType) (
  rate(process_files_rows_total[$__rate_interval])
)
/
sum by (integration, entityType, generationType) (
  rate(process_files_duration_seconds_count[$__rate_interval])
)
