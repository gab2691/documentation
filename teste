1000 *
sum by (integration, entityType, generationType) (
  increase(process_files_duration_seconds_sum[$__interval])
)
/
clamp_min(
  sum by (integration, entityType, generationType) (
    increase(process_files_duration_seconds_count[$__interval])
  ),
  1
)


sum by (integration, entityType, generationType) (
  increase(process_files_rows_total[$__interval])
)


sum by (integration, entityType, generationType) (
  increase(process_files_duration_seconds_count[$__interval])
)
