helm lint ./charts/picpay-ms-v2-qa -f values.qa.yaml

helm template ms-edi-pix ./charts/picpay-ms-v2-qa \
  -f values.qa.yaml \
  --namespace ms-edi-pix > /tmp/render.yaml





apiVersion: v2
name: ms-edi-pix
description: A Helm chart for Kubernetes
type: application
version: 0.1.0
appVersion: 1.16.2

dependencies:
  - name: picpay-ms-v2
    version: "2.12.14"
    repository: "https://chartmuseum.ppay.me"
    condition: picpay-ms-v2.enabled
  - name: picpay-ms-v2-qa
    version: "2.12.14"
    repository: "https://chartmuseum.ppay.me"
    condition: picpay-ms-v2-qa.enabled
