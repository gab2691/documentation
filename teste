@Table(name = "tCompoProvs")
public class ComponenteProvisionado {

    @Id
    @Column(name = "cCompoProvs")
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;

    @Column(name = "cCompoSit", nullable = false)
    private Boolean situacaoComponente;

    @ManyToOne
    @JoinColumn(name = "cCompoEstrt", nullable = false)
    private Componente componente;

    @ManyToOne
    @JoinColumn(name = "cDmandReserEstrtProvs", nullable = false)
    private DemandaReservaProvisionada demandaReservaProvisionada;

    @Column(name = "hDataHoraProvs")
    private LocalDateTime dataHoraProvisionamento;

    @Column(name = "cEmailUsuar", columnDefinition = "VARCHAR(150)")
    private String emailUsuario;

    @Column(name = "cUsuarManut", columnDefinition = "CHAR(9)")
    private String codFuncional;

    @Column(name = "cIdConjCompo")
    private UUID idConjuntoComponente;
}
