fun recordProcessFiles(integration: Integration, entityType: String, generationType: String, elapsedMillis: Long, lines: Int, executionID: String) {
        val tags = Tags.of(
            Tag.of("integration", integration.name),
            Tag.of("entityType", entityType),
            Tag.of("generationType", generationType),
            Tag.of("executionID", executionID)
        )
        meterRegistry.timer(ELAPSED_TIME_KEY, tags)
            .record(elapsedMillis, java.util.concurrent.TimeUnit.MILLISECONDS)

        meterRegistry.counter(LINE_COUNT_KEY, tags)
            .increment(lines.toDouble())
    }
